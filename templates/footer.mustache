{{!
    @template theme_celoe/footer.
    Celoe footer layout template.
}}
<footer id="footer" class="py-3 bg-dark text-light">
  {{#blockarrange}}
  <div class="footer-main">
    <div class="container">
      <div class="row">
        {{#block1}}
        <div class="{{ colclass }}">
          <div class="footer-desc">
            {{#footerlogo}}
            <div class="logo-footer">
              <a href="{{{ config.wwwroot }}}/?redirect=0">
                <img src="{{ logourl }}" alt="Celoe" />
              </a>
            </div>
            {{/footerlogo}}
            <p>{{ footnote }}</p>
          </div>
        </div>
        {{/block1}}
        {{#block2}}
        <div class="{{ colclass }}">
          <div class="footer-contact">
            <h4>{{ footerbtitle2 }}</h4>
            {{# socialurl}}
            <div class="social-media">
              {{# fburl}}
              <a href="{{fburl}}" data-network="facebook" aria-label="Facebook" rel="noopener">
              <svg width="15px" height="15px" viewBox="0 0 20 20" aria-hidden="true">
                <path fill="currentColor" d="M20,10.1c0-5.5-4.5-10-10-10S0,4.5,0,10.1c0,5,3.7,9.1,8.4,9.9v-7H5.9v-2.9h2.5V7.9C8.4,5.4,9.9,4,12.2,4c1.1,0,2.2,0.2,2.2,0.2v2.5h-1.3c-1.2,0-1.6,0.8-1.6,1.6v1.9h2.8L13.9,13h-2.3v7C16.3,19.2,20,15.1,20,10.1z"></path>
              </svg>
              </a>
              {{/ fburl}}
              {{# igurl}}
              <a href="{{igurl}}" data-network="instagram" aria-label="Instagram" rel="noopener">
                <svg width="15px" height="15px" viewBox="0 0 20 20" aria-hidden="true">
                  <circle fill="currentColor" cx="10" cy="10" r="3.3"></circle>
                  <path fill="currentColor" d="M14.2,0H5.8C2.6,0,0,2.6,0,5.8v8.3C0,17.4,2.6,20,5.8,20h8.3c3.2,0,5.8-2.6,5.8-5.8V5.8C20,2.6,17.4,0,14.2,0zM10,15c-2.8,0-5-2.2-5-5s2.2-5,5-5s5,2.2,5,5S12.8,15,10,15z M15.8,5C15.4,5,15,4.6,15,4.2s0.4-0.8,0.8-0.8s0.8,0.4,0.8,0.8S16.3,5,15.8,5z"></path>
                </svg>
              </a>
              {{/ igurl}}
              {{# twurl}}
              <a href="{{twurl}}" data-network="twitter" aria-label="Twitter" rel="noopener">
                <svg width="15px" height="15px" viewBox="0 0 20 20" aria-hidden="true">
                  <path fill="currentColor" d="M20,3.8c-0.7,0.3-1.5,0.5-2.4,0.6c0.8-0.5,1.5-1.3,1.8-2.3c-0.8,0.5-1.7,0.8-2.6,1c-0.7-0.8-1.8-1.3-3-1.3c-2.3,0-4.1,1.8-4.1,4.1c0,0.3,0,0.6,0.1,0.9C6.4,6.7,3.4,5.1,1.4,2.6C1,3.2,0.8,3.9,0.8,4.7c0,1.4,0.7,2.7,1.8,3.4C2,8.1,1.4,7.9,0.8,7.6c0,0,0,0,0,0.1c0,2,1.4,3.6,3.3,4c-0.3,0.1-0.7,0.1-1.1,0.1c-0.3,0-0.5,0-0.8-0.1c0.5,1.6,2,2.8,3.8,2.8c-1.4,1.1-3.2,1.8-5.1,1.8c-0.3,0-0.7,0-1-0.1c1.8,1.2,4,1.8,6.3,1.8c7.5,0,11.7-6.3,11.7-11.7c0-0.2,0-0.4,0-0.5C18.8,5.3,19.4,4.6,20,3.8z"></path>
                </svg>
              </a>
              {{/ twurl}}
              {{# weburl}}
              <a href="{{weburl}}" aria-label="Website" rel="noopener">
                <svg xmlns="http://www.w3.org/2000/svg" width="18px" height="18px" viewBox="0 0 36 36"><path fill="currentColor" d="M10.05 18a20.46 20.46 0 0 0 .62 4.93h6.48v-9.48h-6.57a20.55 20.55 0 0 0-.53 4.55Z" class="clr-i-solid clr-i-solid-path-1"></path><path fill="currentColor" d="M18.85 13.45v9.48h6.48A20.46 20.46 0 0 0 26 18a20.55 20.55 0 0 0-.52-4.55Z" class="clr-i-solid clr-i-solid-path-2"></path><path fill="currentColor" d="M18 2a16 16 0 1 0 16 16A16 16 0 0 0 18 2Zm12.22 22.71H26.6a21.8 21.8 0 0 1-3 6a13.86 13.86 0 0 1-3 .92a20.21 20.21 0 0 0 4.18-6.94h-5.92v7.15h-1.69v-7.13h-5.95a20.21 20.21 0 0 0 4.18 6.95a13.86 13.86 0 0 1-2.94-.9a21.8 21.8 0 0 1-3-6.05H5.78a13.94 13.94 0 0 1-.83-1.81h4a22.2 22.2 0 0 1-.58-4.9a21.88 21.88 0 0 1 .48-4.55H4.76a13.88 13.88 0 0 1 .76-1.81h3.81A22.26 22.26 0 0 1 12.61 5a13.86 13.86 0 0 1 2.87-.84a20.13 20.13 0 0 0-4.4 7.45h6.09V4h1.69v7.64h6.09a20.13 20.13 0 0 0-4.39-7.44a13.89 13.89 0 0 1 2.87.8a22.26 22.26 0 0 1 3.27 6.59h3.77a13.89 13.89 0 0 1 .76 1.81h-4.06a21.88 21.88 0 0 1 .49 4.6a22.2 22.2 0 0 1-.57 4.93h4a13.94 13.94 0 0 1-.87 1.78Z" class="clr-i-solid clr-i-solid-path-3"></path><path fill="none" d="M0 0h36v36H0z"></path></svg>
              </a>
              {{/ weburl}}
              {{# yturl}}
              <a href="{{yturl}}" data-network="youtube" aria-label="YouTube" rel="noopener">
                <svg width="15px" height="15px" viewBox="0 0 20 20" aria-hidden="true">
                  <path fill="currentColor" d="M15,0H5C2.2,0,0,2.2,0,5v10c0,2.8,2.2,5,5,5h10c2.8,0,5-2.2,5-5V5C20,2.2,17.8,0,15,0z M14.5,10.9l-6.8,3.8c-0.1,0.1-0.3,0.1-0.5,0.1c-0.5,0-1-0.4-1-1l0,0V6.2c0-0.5,0.4-1,1-1c0.2,0,0.3,0,0.5,0.1l6.8,3.8c0.5,0.3,0.7,0.8,0.4,1.3C14.8,10.6,14.6,10.8,14.5,10.9z"></path>
                </svg>
              </a>
              {{/ yturl}}
            </div>
            {{/ socialurl}}
            <p>
            {{{ address }}}
            {{# phoneno}}
              <br />
              <span>{{ phoneno }}</span>
            {{/ phoneno}}
            {{# emailid}}
              <br />
              <a class="mail-link" href="mailto:{{ emailid }}">{{ emailid }}</a>
            {{/ emailid}}
            </p>

          </div>
        </div>
        {{/block2}}
        {{#block3}}
        <div class="{{ colclass }}">
          <div class="footer-nav">
            <h4>{{ footerbtitle3 }}</h4>
            <ul>
              {{{footerlinks3}}}
            </ul>
          </div>
        </div>
        {{/block3}}
        {{# block4}}
        <div class="{{ colclass }}">
          <div class="footer-nav">
            <h4>{{ footerbtitle4 }}</h4>
            <ul>
              {{{footerlinks4}}}
            </ul>
          </div>
        </div>
        {{/ block4}}
      </div>
    </div>
  </div>
  {{/blockarrange}}
  {{#copyright}}
  <div class="footer-foot">
    <div class="container">{{{copyright}}}</div>
  </div>
  {{/copyright}}
</footer>

<footer>
  {{{output.standard_footer_html}}}

  {{# output.page_doc_link }}
  <p class="helplink">{{{ output.page_doc_link }}}</p>
  {{/ output.page_doc_link }}

  {{{ output.standard_end_of_body_html }}}
</footer>

{{#js}}
require(['theme_boost/loader']); require(['theme_boost/drawer'], function(mod) {
mod.init(); });
{{/js}}
